# Sanity CMS Configuration
# Copy this file to .env and fill in your actual values

# Your Sanity project ID (found in your Sanity project settings)
VITE_SANITY_PROJECT_ID=your_project_id_here

# Dataset name (typically 'production' or 'development')
VITE_SANITY_DATASET=production

# API version (optional, defaults to 2024-01-01 if not set)
# Use YYYY-MM-DD format
VITE_SANITY_API_VERSION=2024-01-01

# Sanity Studio Configuration (for running Sanity Studio locally)
# These should match your VITE_ variables above
SANITY_STUDIO_PROJECT_ID=your_project_id_here
SANITY_STUDIO_DATASET=production
